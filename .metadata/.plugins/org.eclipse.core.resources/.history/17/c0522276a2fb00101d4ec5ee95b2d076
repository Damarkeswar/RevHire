package com.revhire.dao.test;

import com.revhire.dao.UserDao;
import com.revhire.dao.UserDaoImpl;
import com.revhire.model.User;

public class UserDaoTest {

    public static void main(String[] args) {

        UserDao userDao = new UserDaoImpl();

        // TEST 1: LOGIN
        User user = userDao.login("js1@gmail.com", "js123");

        if (user != null) {
            System.out.println("✅ DAO Login Test Passed");
        } else {
            System.out.println("❌ DAO Login Test Failed");
        }
    }
}
