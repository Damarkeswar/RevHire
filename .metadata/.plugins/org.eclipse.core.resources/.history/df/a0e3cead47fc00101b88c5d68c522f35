package com.revhire.dao;

import java.util.List;

public interface JobDao {

	boolean postJob(Job job);

	List<Job> getAllJobs();
	List<Job> searchJobs(
		    String title,
		    String location,
		    Integer minExperience,
		    String companyName,
		    Double minSalary,
		    String jobType
		);

}
